<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jsmiranda.acad.firebase_login"
    >
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Register"
            android:label="@string/app_register"
            >
        </activity>
        <activity
            android:name=".User_Dashboard"
            android:label="@string/app_user_dashboard"
            >

        </activity>
        <activity android:name=".Admin_Dashboard"
            android:label="@string/app_admin_dashboard"
         ></activity>
    </application>

</manifest>